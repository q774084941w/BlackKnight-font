wx-body{ background: #F2F2F2; }
.switch-panel{ width: 100%; height: 100%; position: relative; font-size: 30rpx; }
.switch-panel-title{ background: #FFF; color: #333; display: -webkit-flex; display: flex; height: 100rpx; line-height: 100rpx; }
.switch-panel-title .active{ border-bottom: 1rpx solid #17AB1B; color: #17AB1B; }
.switch-panel-title-item{ -webkit-flex: 1; flex: 1; text-align: center; }
.switch-panel-swiper{ width: 100%; height: 1000rpx; }
.switch-panel-content{ width: 100%; height: 100%; padding-bottom: 100rpx; }
.order-module{ background: #FFF; border-radius: 10rpx; width: 90%; margin: 20rpx auto; padding: 10rpx 2.5%; box-shadow: 0 0 10rpx #ccc; overflow: hidden; }
.order-module .inline{ display: -webkit-flex; display: flex; font-size: 28rpx; padding: 15rpx 0; }
.title{ font-weight: bold; width: 100%;}
.order-module .label{ color: #111;min-width: 160rpx; }
.order-module .info{ color: #666; }
.none{ display: none; }
.statistics{ width: 40rpx;      height: 40rpx;      background-color:red;      border-radius: 50%;      -moz-border-radius: 50%;      -webkit-border-radius: 50%;position:relative;line-height:40rpx;font-size:20rpx;color:white;top:-85rpx;left:106rpx;}
.statistics2{ width: 25rpx;      height: 25rpx;      background-color:red;      position:relative;line-height:25rpx;font-size:20rpx;color:white;top:-59rpx;left:120rpx;}
.time_load {float:right}